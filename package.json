    "ejs-loader": "^0.3.1",
    "glob": "^7.1.2",
    "react": "^16.3.2",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.10",
    "webpack-serve": "^0.2.0"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=8.0.0",
    "yarn": ">=1.0.0"
  },
  "private": true,
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1"
  }
}
